@*@model IEnumerable<WebApplication10.pepole>*@
<link href="https://cdn.datatables.net/1.10.20/css/dataTables.bootstrap.min.css" rel="stylesheet" />
<script src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js"></script>
<script src="https://cdn.datatables.net/1.10.20/js/dataTables.bootstrap.min.js"></script>
<style>
   
</style>
<h2>List of Pepole Into PDF</h2>

<table class="table table-bordered table-hover" style="background-color:khaki;border:5px solid red">
    <tr>
        <th>
            @*@Html.DisplayNameFor(model => model.ID)*@
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Name)*@
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.Email)*@
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                @*@Html.DisplayFor(modelItem => item.ID,new {@class= "text-danger" })*@
            </td>
            <td>
                @*@Html.DisplayFor(modelItem => item.Name)*@
            </td>
            <td>
                @*@Html.DisplayFor(modelItem => item.Email)*@
            </td>
        </tr>
    }

</table>
<script></script>