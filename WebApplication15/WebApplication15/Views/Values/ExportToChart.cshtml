<script>
 

    $(document).ready(function () {

        var chart = new CanvasJS.Chart("chart", {
            zoomEnabled: true,
            exportEnabled: true,
            animationEnabled: true,
            title: { text: "نمودار خطی   " },
            data: [{ type: "line", dataPoints: @Html.Raw(ViewBag.DataPoints)}]
        });
        chart.render();
        document.getElementById("TypeOfChart").innerHTML = "خطی";
     });

    $("#abshary").click(function () {

        var chart = new CanvasJS.Chart("chart", {
            zoomEnabled: true,
            exportEnabled: true,
            animationEnabled: true,
            title: { text: "نمودار خطی   " },
            data: [{ type: "line", dataPoints: @Html.Raw(ViewBag.DataPoints)}]
        });
        chart.render();
        document.getElementById("TypeOfChart").innerHTML = "خطی";
     });


    $("#sotoni").click(function () {

        var chart = new CanvasJS.Chart("chart", {
            animationEnabled: true,
            exportEnabled: true,
            title: { text: "نمودار میله ای" },
            data: [{ type: "column", dataPoints: @Html.Raw(ViewBag.DataPoints)
    }]
            });
        chart.render();
        document.getElementById("TypeOfChart").innerHTML = "میله ای";

        });


    $("#sotoni2").click(function () {
        var chart = new CanvasJS.Chart("chart", {
            theme: "light1", // "light1", "light2", "dark1", "dark2"
            exportEnabled: true,
            animationEnabled: true,
            title: { text: "نمودار دایره ای  " },
            subtitles: [{ text: "" }],
            data: [{
                type: "pie",
                startAngle: 180,
                toolTipContent: "<b>زمان: </b> ساعت {x} <br/><b>دما: </b> {y} درجه",
                showInLegend: "true",
                legendText: "{label}",
                indexLabel: "{label} - {y}%",
                dataPoints: @Html.Raw(ViewBag.DataPoints)
    }]
            });
        chart.render();
        document.getElementById("TypeOfChart").innerHTML = "دایره ای";

        });


    $("#sotoni3").click(function () {

        var chart = new CanvasJS.Chart("chart", {
            theme: "light2",
            animationEnabled: true,
            title: { text: "مساحت سطح زیر نمودار" },
            axisX: { title: "(زمان (ساعت" },
            axisY: { title: "(دما (درجه " },
            data: [{
                type: "area",
                color: "#0BB5FF",
                markerSize: 0,
                toolTipContent: "زمان:  ساعت {x} <br>دما: {y} درجه",
                dataPoints: @Html.Raw(ViewBag.DataPoints)
    }]
            });
        chart.render();
